{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "make-build",
			"type": "process",
			"command": "make",
			"args": [
				"build",
				"DEBUG=1"
			],
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "make-rebuild",
			"type": "process",
			"command": "make",
			"args": [
				"rebuild",
				"DEBUG=1"
			]
		},
		{
			"label": "make-clean",
			"type": "process",
			"command": "make",
			"args": [
				"clean"
			]
		}
	]
}